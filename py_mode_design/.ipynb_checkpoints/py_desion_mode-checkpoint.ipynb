    "class sign_brogA(brogA):\n",
    "    \n",
    "    def __init__(self):\n",
    "        brogA.__init__(self)   # 此步确保状态共享\n",
    "        self.sta = 'init'\n",
    "    def __str__(self):\n",
    "        return self.sta"
