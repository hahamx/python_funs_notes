    "    \n",
    "    try:\n",
    "        start = time.time()\n",
    "        yield\n",
    "    finally:\n",
    "        end = (time.time() - start)*1000\n",
    "        print('time taken=> %.2f ms' % end)\n",
    "        \n",
    "def max_subarray(sequence):\n",
    "    \"\"\" Maximum subarray - optimized version \"\"\"\n",
    "\n",
    "    max_here = max_far = 0\n",
    "    for x in sequence:\n",
